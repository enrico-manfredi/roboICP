    "import serial\n",
    "import time\n",
    "if __name__ == '__main__':\n",
    "    ser = serial.Serial('COM5', 9600, timeout=1)\n",
    "    ser.flush()\n",
    "    while True:\n",
    "        ser.write(b\"Hello from Raspberry Pi!\\n\")\n",
    "        line = ser.readline().decode('utf-8').rstrip()\n",
    "        print(line)\n",
    "        time.sleep(1)"
